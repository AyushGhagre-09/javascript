node {
   stage('Cleanup') {
    cleanWs(
        deleteDirs: true,
        patterns: [
            [pattern: 'src/Maestro/node_modules/**', type: 'EXCLUDE']
        ]
    )
}

    
  stage('Checkout and Install') {
