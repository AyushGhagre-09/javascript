    "scripts": {
        "apollo-schema": "apollo service:download -c apollo.config.js ",
        "apollo-codegen-linux": "wsl apollo client:codegen app/generatedGraphQLTypes -c apollo.config.js --target=typescript --outputFlat",
        "apollo-codegen": "rimraf app/generatedGraphQLTypes/* && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 apollo client:codegen app/generatedGraphQLTypes -c apollo.config.js --target=typescript --outputFlat --passthroughCustomScalars --customScalarsPrefix=GraphQL_",
        "apollo-codegen-watch": "apollo client:codegen app/generatedGraphQLTypes -c apollo.config.js --target=typescript --watch --outputFlat",
        "apollo-codegen-watch-linux": "wsl apollo client:codegen app/generatedGraphQLTypes -c apollo.config.js --target=typescript --watch --outputFlat",
        "fetch-geodata": "node ./app/components/Map/geodata/script/fetch-geodata.js",
        "fetch-geodata:debugger": "node --inspect-brk ./app/components/Map/geodata/script/fetch-geodata.js --runInBand --env=jsdom",
        "lint:prettier": "prettier --check **/*",
        "lint:prettier:fix": "prettier --write **/*",
        "lint:prettier:staged": "lint-staged",
        "eslint:fix": "cross-env NODE_OPTIONS=--max_old_space_size=8192 eslint -p . -c --quiet --cache --fix",
        "eslint:all": "cross-env NODE_OPTIONS=--max_old_space_size=8192 npm-run-all --aggregate-output --print-label --continue-on-error --parallel eslint:app",
        "eslint:app": "eslint \"./app/**/*.{ts,tsx}\" --quiet --cache --cache-location \"app.eslintcache\"",
        "eslint:wptApp": "eslint \"./wpt-app/**/*.{ts,tsx}\" --quiet --cache --cache-location \"wptApp.eslintcache\"",
        "eslint:jenkins": "cross-env NODE_OPTIONS=--max_old_space_size=8192 npm-run-all --aggregate-output --print-label --continue-on-error --parallel eslint:app:jenkins",
        "eslint:app:jenkins": "eslint \"./app/**/*.{ts,tsx}\" --quiet",
        "eslint:wptApp:jenkins": "eslint \"./wpt-app/**/*.{ts,tsx}\" --quiet",
        "lint:sass": "stylelint app/**/*.scss --cache",
        "lint:styles": "npm-run-all --aggregate-output --print-label --parallel lint:sass lint:styled-components",
        "lint:styled-components": "stylelint app/**/*.{ts,tsx} --config .stylelintrc.styled.json --cache",
        "lint:staged": "npm-run-all --aggregate-output --print-label --parallel eslint:all lint:styles",
        "test": "jest --env=jsdom-sixteen --config jest.config.js",
        "test:silent": "cross-env JEST_SILENT_REPORTER_DOTS=true JEST_SILENT_REPORTER_SHOW_PATHS=true jest --env=jsdom-sixteen --config jest.config.js --reporters=jest-silent-reporter",
        "test:log-dom": "cross-env DEBUG_PRINT_LIMIT=1000000 jest --env=jsdom-sixteen --config jest.config.js",
        "test:generate-output": "jest --env=jsdom-sixteen --config jest.config.js --json --outputFile=.jest-test-results.json",
        "test:quick": "node testChangedFiles.js",
        "test:debugger": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --env=jsdom --config jest.config.js",
        "test:coverage": "jest --env=jsdom-sixteen --coverage --config jest.config.js --no-cache",
        "storybook": "cross-env NODE_OPTIONS=--max_old_space_size=8192 start-storybook -p 6006",
        "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --simple=true",
        "build-with-type-check": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --typeCheck=true",
        "build:sa": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --isStandAlone=true --standAloneLocalPort=3000 --standAloneApiEndpoint=https://portalstage.catchpoint.com/m/g --standAloneConductorUrl=https://iostage.catchpoint.com/auth",
        "build:tracing": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --isStandAlone=true --standAloneLocalPort=3000 --standAloneApiEndpoint=https://portalstage.catchpoint.com/m/g --standAloneConductorUrl=https://iostage.catchpoint.com/auth --no-cache",
        "start": "cd ../../node-server && yarn start && cd src/Maestro/",
        "open": "cd ../../node-server && yarn open && cd src/Maestro/",
        "build:wpt": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --config webpack.wpt.config.js",
        "build:pretty": "webpack-dashboard -- cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --prettyDisplay=true",
        "build-storybook": "cross-env NODE_OPTIONS=--max_old_space_size=8192 build-storybook -o .sbstatic",
        "build:jenkins": "cross-env NODE_OPTIONS=--max_old_space_size=8192 npm-run-all --aggregate-output --print-label --continue-on-error --parallel lint:es lint:sass test webpack:dev",
        "build:jenkins-prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 npm-run-all --aggregate-output --print-label --continue-on-error --parallel lint:es lint:sass test webpack:prod",
        "webpack:dev": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --simple=true --release=%COMPUTERNAME%",
        "webpack:prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --mode=production",
        "webpack:graphiql": "webpack --config webpack-graphiql.config.js",
        "webpack:hawkui": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --config webpack.hawkui.config.js",
        "webpack:hawkui-prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config webpack.hawkui.config.js --mode=production",
        "webpack:wpt": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config webpack.wpt.config.js --simple=true",
        "webpack:wpt-prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config webpack.wpt.config.js --mode=production",
        "deploy-storybook": "cross-env NODE_OPTIONS=--max_old_space_size=8192 storybook-to-ghpages -- --source-branch=develop",
        "deploy-storybook-jenkins": "cross-env NODE_OPTIONS=--max_old_space_size=8192 storybook-to-ghpages -- --source-branch=develop --existing-output-dir=.sbstatic --ci",
        "post-install": "rimraf -- .cache-loader .awcache node_modules/.cache",
        "clean:cache": "rimraf .cache-loader .awcache node_modules/.cache",
        "webpack-stats": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --mode=production --profile --json > stats.json",
        "webpack-stats-wpt": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --progress --config webpack.wpt.config.js --mode=production --profile --json > stats.wpt.json",
        "webpack-analyze": "webpack-bundle-analyzer pathToStatsFile",
        "graphql-gen": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen",
        "codegen": "npm-run-all graphql-gen apollo-codegen",
        "generate-queries": "ts-node generateGraphqlQueries.ts",
        "generate-queries-jenkins": "ts-node generateGraphqlQueries.ts --mode=production"
    },
    "lint-staged": {
        "*": "prettier --write"
    }
}
